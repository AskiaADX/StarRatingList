<?xml version="1.0" encoding="utf-8"?>
<control  xmlns="http://www.askia.com/2.1.0/ADCSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.askia.com/2.1.0/ADCSchema https://raw.githubusercontent.com/AskiaADX/ADXSchema/2.1.0/ADCSchema.xsd"
          version="2.1.0"
          askiaCompat="5.4.2">
  <info>
    <name>StarRatingList</name>
    <guid>995b8259-2cec-4368-b733-d7ea6eec68a9</guid>
    <version>2.0.2</version>
    <date>2014-07-03</date>
    <description><![CDATA[v1.0.1 - Fixed error thrown in IE8 because of old & unused code
		v1.0.2 - Fixed empty element in itteration loop when numeric question (see github change)
		v1.0.3 - Fixed a wrong loop iteration count
		v1.0.4 - JD - Fixed encoding of caption
		           - Fixed hover problem on mobile/tablet
		v1.0.5 - JD - Added autoForwardLastIteration parameter to go to the next screen after the last iteration
		v1.0.6 - JD - Added use parameter
		v1.0.7 - JD - Fixed dk hover when selected
		v1.0.8 - JD - Fixed element name
		v2.0.0 - JD - Theme implementation]]></description>
    <company>Askia</company>
    <author><![CDATA[Paul Ferreira <paulf@askia.com>        Jerome Duparc <jeromed@askia.com>]]></author>
    <site>http://www.askia.com</site>
    <helpURL>https://dev.askia.com</helpURL>
    <categories>
      <category>General</category>
    </categories>
    <constraints>
      <constraint on="questions" single="true" numeric="true" requireParentLoop="true" chapter="false" date="false" multiple="false" open="false" />
      <constraint on="controls" textbox="true" responseblock="true" checkbox="false" label="false" listbox="false" radiobutton="false" />
      <constraint on="responses" max="*" min="*" />
    </constraints>
  </info>
  <outputs defaultOutput="fallback">
    <output id="standard">
      <description><![CDATA[Output when the browser support Javascript]]></description>
      <condition><![CDATA[Browser.Support("javascript")]]></condition>
      <content fileName="starratingList.css" type="css" mode="static" position="head" />
      <content fileName="standard.css" type="css" mode="dynamic" position="head" />
      <content fileName="standard.html" type="html" mode="dynamic" position="placeholder" />
      <content fileName="standard_single.html" type="html" mode="dynamic" position="none" />
      <content fileName="standard_numeric.html" type="html" mode="dynamic" position="none" />
      <content fileName="es5-shim.min.js" type="javascript" mode="share" position="head">
        <yield><![CDATA[
          
          <!--[if lt IE 10]>
            <script type="text/javascript" src="{%= CurrentADC.URLTo("share/es5-shim.min.js") %}"></script>
          <![endif]-->
          
        ]]></yield>
      </content>
      <content fileName="domready.js" type="javascript" mode="share" position="head" />
      <content fileName="tippy.css" type="css" mode="share" position="head" />
      <content fileName="tippy.min.js" type="javascript" mode="share" position="head" />
      <content fileName="starratingList.js" type="javascript" mode="static" position="head" />
      <content fileName="standard.js" type="javascript" mode="dynamic" position="foot" />
      <content fileName="standard_single.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="standard_numeric.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="star.png" type="image" mode="static" position="none" />
      <content fileName="next.png" type="image" mode="static" position="none" />
      <content fileName="prev.png" type="image" mode="static" position="none" />
      <content fileName="heart.png" type="image" mode="static" position="none" />
      <content fileName="sun.png" type="image" mode="static" position="none" />
      <content fileName="thumb.png" type="image" mode="static" position="none" />
      <content fileName="tick.png" type="image" mode="static" position="none" />
    </output>
    <output id="fallback">
      <description><![CDATA[Fallback when the browser doesn&acute;t support Javascript]]></description>
      <condition><![CDATA[Not(Browser.Support("javascript"))]]></condition>
      <content fileName="default.css" type="css" mode="static" position="head" />
      <content fileName="fallback_single_horizontal.html" type="html" mode="dynamic" position="none" />
      <content fileName="fallback_single_vertical.html" type="html" mode="dynamic" position="none" />
      <content fileName="fallback_numeric.html" type="html" mode="dynamic" position="none" />
      <content fileName="default.html" type="html" mode="dynamic" position="placeholder" />
    </output>
  </outputs>
